package vgbox.vgboxDB.VO;

import java.util.Date;

public class Product {
	private int product_num;
	private String id;
	private String product_name;
	private int price;
	private Date reservation_period;
	private int for_sale;
	private String img;

	public Product() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Product(int product_num, String id, String product_name, int price, Date reservation_period, int for_sale,
			String img) {
		super();
		this.product_num = product_num;
		this.id = id;
		this.product_name = product_name;
		this.price = price;
		this.reservation_period = reservation_period;
		this.for_sale = for_sale;
		this.img = img;
	}

	public int getProduct_num() {
		return product_num;
	}

	public void setProduct_num(int product_num) {
		this.product_num = product_num;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getProduct_name() {
		return product_name;
	}

	public void setProduct_name(String product_name) {
		this.product_name = product_name;
	}

	public int getPrice() {
		return price;
	}

	public void setPrice(int price) {
		this.price = price;
	}

	public Date getReservation_period() {
		return reservation_period;
	}

	public void setReservation_period(Date reservation_period) {
		this.reservation_period = reservation_period;
	}

	public int getFor_sale() {
		return for_sale;
	}

	public void setFor_sale(int for_sale) {
		this.for_sale = for_sale;
	}

	public String getImg() {
		return img;
	}

	public void setImg(String img) {
		this.img = img;
	}

}